              "requests": [
                {
                  "response_format": "timeseries",
                  "queries": [
                    {
                      "data_source": "metrics",
                      "name": "query1",
                      "query": "system.cpu.idle{*}"
                    },
                    {
                      "data_source": "metrics",
                      "name": "query2",
                      "query": "system.cpu.system{*}"
                    },
                    {
                      "data_source": "metrics",
                      "name": "query3",
                      "query": "system.cpu.iowait{*}"
                    },
                    {
                      "data_source": "metrics",
                      "name": "query4",
                      "query": "system.cpu.user{*}"
                    },
                    {
                      "data_source": "metrics",
                      "name": "query5",
                      "query": "system.cpu.stolen{*}"
                    },
                    {
                      "data_source": "metrics",
                      "name": "query6",
                      "query": "system.cpu.guest{*}"
                    }
                  ]
                }
              ]
            },
            "layout": {
              "x": 0,
              "y": 0,
              "width": 3,
              "height": 2
            }
          },
