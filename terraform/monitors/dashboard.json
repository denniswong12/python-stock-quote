              "requests": [
                {
                  "response_format": "timeseries",
                  "queries": [
                    {
                      "data_source": "metrics",
                      "name": "query1",
                      "query": "sum:system.mem.usable{*}"
                    },
                    {
                      "data_source": "metrics",
                      "name": "query2",
                      "query": "sum:system.mem.total{*}"
                    }
                  ],
                  "formulas": [
                    {
                      "formula": "query1"
                    },
                    {
                      "formula": "query2 - query1"
                    }
                  ]
                }
              ]
            },
            "layout": {
              "x": 3,
              "y": 0,
              "width": 3,
              "height": 2
            }
